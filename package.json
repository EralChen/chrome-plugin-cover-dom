	},
	"dependencies": {
		"@element-plus/utils": "workspace:*",
		"@vueuse/core": "^8.7.5",
		"@vunk-skzz/components": "workspace:*",
		"@vunk-skzz/shared": "workspace:*",
		"@vunk/core": "0.0.0-alpha.2",
