		"@lib-env/build-constants": "workspace:*",
		"@lib-env/build-utils": "workspace:*",
		"@lib-env/eslint-config": "workspace:*",
		"@lib-env/path": "workspace:*",
		"@lib-env/shared": "workspace:*",
		"@types/gulp": "^4.0.9",
		"@types/node": "^18.0.4",
		"consola": "^2.15.3",
		"eslint": "^8.19.0",
		"eslint-define-config": "^1.5.1",
		"eslint-plugin-vue": "^9.2.0",
		"fast-glob": "^3.2.11",
		"gulp": "^4.0.2",
